package com.sonata.Model;
import java.util.Date;

import javax.persistence.Entity;
import javax.persistence.Id;
@Entity
public class User {
	@Id
	long User_ID;
	String Username;
	String Email;
	long Contact;
	String Role;
	boolean IsActive; 
	Date DOB;
	Date Created_On;
	String Password;
	User()
	{
		
	}
	
	
	
}